{
  "name": "MALVIN-XD",
  "description": "MALVIN XD Whatsapp bot © ᴄʀᴇᴀᴛᴇᴅ ʙʏ ᴍᴀʟᴠɪɴ ᴋɪɴɢ · · ·",
  "logo": "https://files.catbox.moe/7pg2gp.jpg",
  "keywords": ["bot"],
  "success_url": "/",

    "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "Put the session-id here.",
      "required": true,
      "value": ""
    },  
    
    "PREFIX": {
      "description": "Set bot prefix(#,$,-!,?)",
      "required": true,
      "value": "."
    },
    
    "MODE": {
      "description": "select your bot work type public-private-inbox-group.",
      "required": true,
      "value": "public"
    }, 

    "AUTO_VOICE": {
      "description": "Make it true if you want bot auto voice.",
      "required": true,
      "value": "true"
    },  

    "AUTO_READ_STATUS": {
      "description": "Make it true if you want bot auto auto_read_status.",
      "required": true,
      "value": "true"
    }, 
    
    "STATUES_REPLY": {
      "description": "Statues reply voice",
      "required": true,
      "value": "false"
    },
    
    "FAKE_RECORDING": {
      "description": "Make it true if you want bot fake record.",
      "required": true,
      "value": "true"
    },
    
    "AUTO_TYPING": {
      "description": "Make it true if you want bot auto typing",
      "required": true,
      "value": "true"
    },

    "AUTO_REACT": {
      "description": "Make it true if you want bot AutoReact.",
      "required": true,
      "value": "false"
    },
    
    "ANTI_LINK": {
      "description": "Group link shear not allowed and kick out user",
      "required": true,
      "value": "true"
    },
    
    "ANTI_BAD": {
      "description": "Bad word not allowed",
      "required": true,
      "value": "true"
    },
    
    "READ_MESSAGE": {
      "description": "Auto read new messages",
      "required": true,
      "value": "false"
    },
    
    "ALWAYS_ONLINE": {
      "description": "Make it true if show you are always online it false if show your last seen",
      "required": true,
      "value": "true"
    },
    
    "ANTI_DELETE": {
      "description": "Delete massages show",
      "required": true,
      "value": "true"
    },
    
    "INBOX_BLOCK": {
      "description": "Inbox Auto block",
      "required": true,
      "value": "false"
    }

},

     "buildpacks": [
        {
            "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
        }
     ],
  "stack": "heroku-24"
}
